<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="VWDocTypeForInUnsubmitQVO"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="erpsolims.erpsolimsmodel.erpsolimsqvo.VWDocTypeForInUnsubmitQVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P_ADF_PROJECTID"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="P_ADF_USERID"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[select m.DocTypeId , m.module_name   
  from sys_program m, sys_users_detail d    
 where m.moduleid = d.moduleid    
   and nvl(d.unsubmit, 'N') = 'Y'    
   and m.DocTypeId is not null    
   and m.ProjectId = :P_ADF_PROJECTID    
   and d.userid = :P_ADF_USERID]]></SQLQuery>
  <ViewAttribute
    Name="Doctypeid"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOCTYPEID"
    Expression="DOCTYPEID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="15"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ModuleName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="75"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MODULE_NAME"
    Expression="MODULE_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="75"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="Doctypeid"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="erpsolims.erpsolimsmodel.ERPSolIMSModelBundle"/>
  </ResourceBundle>
</ViewObject>
